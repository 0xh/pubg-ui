{
  "name": "pubg-ui",
  "version": "1.0.0",
  "private": true,
  "description": "PUBG UI",
  "main": "build/index.js",
  "author": "Cody Mathisen",
  "license": "MIT",
  "scripts": {
    "debug-server": "python packages/debug-server/debug-server.py",
    "react:start": "react-scripts-ts start",
    "react:build": "react-scripts-ts build",
    "react:test": "react-scripts-ts test --env=jsdom",
    "react:eject": "react-scripts-ts eject",
    "start:dev": "concurrently \"yarn react:start\" \"http-server null -p 80 -P http://localhost:3000\"",
    "start:debug": "concurrently \"yarn debug-server\" \"http-server null -p 80 -P http://localhost:3001\"",
    "start:game": "http-server null -p 80 -P https://prod-live-front.playbattlegrounds.com"
  },
  "dependencies": {
    "react-scripts-ts": "2.6.0"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.33",
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.10",
    "concurrently": "^3.5.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-observable": "^0.16.0",
    "rxjs": "^5.4.3",
    "styled-components": "^2.2.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
